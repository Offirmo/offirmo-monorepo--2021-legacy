{
	"name": "@atlassian/log-experiment-to-console-browser",
	"description": "output a XBL experiment to console",
	"version": "0.0.1",
	"author": "Yves Jutard <yjutard@atlassian.com>",
	"license": "UNLICENSED",
	"private": true,

	"esnext": "dist/src.es9/index.js",
	"module": "dist/src.es9/index.js",
	"main": "dist/src.es9.cjs/index.js",
	"typings": "dist/src.es9.cjs/index.d.ts",
	"source": "src/index.ts",
	"sideEffects": false,

	"peerDependencies": {
		"tslib": "^1"
	},
	"dependencies": {
		"@atlassian/experiment-bootstrap": "^0.2.0"
	},

	"scripts": {
		"clean": "node ../../scripts/clean.js dist",

		"xxtest": "../../node_modules/.bin/mocha --opts ../../3-tools/unit-test-toolbox/mocha.opts ../../3-tools/unit-test-toolbox/mocha-chai-init-node.js 'dist/src.es9.cjs/**/*spec.js'",

		"build:dev:watch": "node ../../scripts/build-typescript.js --watch",
		"build:prod": "node ../../scripts/build-typescript.js",

		"dev": "run-s clean build:dev:watch",
		"xxbuild": "run-s build:prod",
		"demo": "parcel doc/demo/index.html"
	},

	"devDependencies": {
		"bowser": "^1",
		"npm-run-all": "^4",
		"react-dom": "^16",
		"react-loadable": "^5"
	}
}
