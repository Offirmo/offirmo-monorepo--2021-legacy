{
	"name": "@atlassian/experiment-bootstrap",
	"description": "Growth experiments bootstrap logic",
	"version": "0.2.0",
	"license": "UNLICENSED",
	"author": "Atlassian Pty Ltd",
	"maintainers": [
		"Yves J"
	],

	"esnext": "dist/src.es9/index.js",
	"module": "dist/src.es9/index.js",
	"main": "dist/src.es9.cjs/index.js",
	"typings": "dist/src.es9.cjs/index.d.ts",
	"source": "src/index.ts",
	"sideEffects": false,

	"size-limit": [
		{
			"limit": "5kb",
			"path": "dist/src.es9/index.js"
		},
		{
			"limit": "5kb",
			"path": "dist/src.es9.cjs/index.js"
		}
	],

	"peerDependencies": {
	},
	"dependencies": {
		"@offirmo/globalthis-ponyfill": "^0.0.4",
		"@offirmo/universal-debug-api-minimal-noop": "^0.0.2",
		"tiny-invariant": "^1",
		"tslib": "^1",
		"typescript-string-enums": "^1"
	},

	"devDependencies": {
		"npm-run-all": "^4",
		"parcel-bundler": "^1",
		"react-dom": "^16",
		"react-loadable": "^5",
		"size-limit": "^1"
	},
	"scripts": {
		"clean": "node ../../scripts/clean.js dist",

		"test": "../../node_modules/.bin/mocha --opts ../../3-tools/unit-test-toolbox/mocha.opts ../../3-tools/unit-test-toolbox/mocha-chai-init-node.js 'dist/src.es9.cjs/**/*spec.js'",

		"build:dev:watch": "node ../../scripts/build-typescript.js --watch",
		"build:prod": "node ../../scripts/build-typescript.js",
		"ensure-size": "size-limit",

		"dev": "run-s clean build:dev:watch",
		"build": "run-s build:prod",
		"demo": "parcel doc/demo/index.html",

		"prepublishOnly": "run-s clean build:prod ensure-size"
	},

	"files": [
		"dist",
		"src"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Offirmo/offirmo-monorepo.git"
	},
	"keywords": [
		"logger",
		"typescript"
	],
	"homepage": "https://github.com/Offirmo/offirmo-monorepo/tree/master/4-incubator/experiment-bootstrap"
}
