{
	"name": "@oh-my-rpg/view-browser",
	"version": "0.0.1",
	"private": true,
	"description": "Graphical css and assets for an RPG UI",
	"license": "UNLICENSED",
	"devDependencies": {
		"@offirmo/view-css": "^0.0.1",
		"clean-css-cli": "^4.1.10",
		"cpx": "^1.3.2",
		"normalize.css": "^8.0.0",
		"npm-run-all": "^4.1.2"
	},
	"scripts": {
		"clean": "rm -rf dist",
		"build:mkdir": "mkdir -p dist",
		"build:copy": "cpx 'src/**/*.{woff,woff2,eot,svg,ttf,png,jpg}' dist --verbose",
		"build:cleancss": "cleancss --skip-rebase -o dist/oh-my-rpg-browser.css src/style.css",
		"build": "npm-run-all clean build:mkdir --parallel build:copy build:cleancss"
	}
}
